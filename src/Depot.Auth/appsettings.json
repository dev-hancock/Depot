{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "Depot",
    "Audience": "DepotClients",
    "KeyPath": "../../keys/jwt_private.pem",
    "AccessTokenLifetime": 60,
    "RefreshTokenLifetime": 240,
    "Key": "jwt_private.pem"
  }
}
